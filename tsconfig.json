{
	"compilerOptions": {
		"target": "es2020",
		"outDir": "./dist",
		"rootDir": "./src",
		"module": "CommonJS",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src/swagger-output.json", "src/**/*"]
}
